{
  "name": "podcastdx-client",
  "version": "5.0.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/RyanHirsch/podcastdx-client"
  },
  "scripts": {
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "yarn prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "node dist/index.js",
    "prepublishOnly": "yarn lint && yarn test && yarn build && yarn validate && ./scripts/publish.sh",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --ext .js,.ts src/",
    "test:watch": "jest --watch",
    "test:episodes": "DEBUG=* jest episodes",
    "test:podcasts": "DEBUG=* jest podcasts",
    "test:recent": "DEBUG=* jest recent",
    "test:search": "DEBUG=* jest search",
    "validate": "DEBUG=* ts-node -r tsconfig-paths/register src/schemas/validate.ts",
    "dev": "DEBUG=* ts-node -r tsconfig-paths/register example.ts",
    "dev:watch": "ts-node-dev --respawn --no-notify example.ts"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/node-fetch": "^2.5.8",
    "@types/ramda": "^0.27.38",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "auto-changelog": "^2.2.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^5.1.2",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.28",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "ajv": "^7.1.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "isomorphic-fetch": "^3.0.0",
    "ramda": "^0.27.1",
    "typescript-json-schema": "^0.49.0"
  }
}
