{
  "name": "podcastdx-client",
  "version": "2.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/RyanHirsch/podcastdx-client"
  },
  "scripts": {
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "yarn prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "node dist/index.js",
    "prepublishOnly": "yarn lint && yarn test && yarn build && ./scripts/publish.sh",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --ext .js,.ts src/",
    "test:watch": "jest --watch",
    "dev": "DEBUG=* ts-node -r tsconfig-paths/register example.ts",
    "dev:watch": "ts-node-dev --respawn --no-notify example.ts"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/node-fetch": "^2.5.7",
    "@types/ramda": "^0.27.17",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "auto-changelog": "^2.2.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1"
  }
}
